#include<stdio.h>
int main()
{
    int n,i;
    scanf("%d",&n);
    int a[n];
    int b[100];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        if(a[i]>0)
        {
            b[i]=-a[i];
            printf("%d ",a[i]);
            printf("%d ",b[i]);
        }
    }
    
    return 0;
}

