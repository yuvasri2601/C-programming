#include <stdio.h>
#include <stdlib.h>

char check(char c)
{
    if(c>=65 && c<=90)
        printf("Upper Case");
    else if(c>=97 && c<=122)
        printf("Lower Case");
    else
        printf("Invalid");
    return 0;

    //Type your code here
}

int main()
{
  char c,result;
    scanf("%c",&c);
    result=check(c);
    //Type your code here
    return 0;
}
