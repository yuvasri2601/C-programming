#include<stdio.h>
int main()
{
    int n,arr[100],i;
    printf("Enter the element: ");
    scanf("%d",&n);
    printf("Enter the array elements:\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    printf("The reversed array is: \n");
    for(i=n-1;i>=0;i--)
    {
        printf("%d\n",arr[i]);
    }
   return 0;
}
